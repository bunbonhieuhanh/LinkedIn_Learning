{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "minLength": 10,
      "maxLength": 125
    },
    "price": {
      "type": "biginteger"
    },
    "duration": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "richtext",
      "minLength": 20,
      "maxLength": 225
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "course"
    },
    "level": {
      "type": "string"
    },
    "enrollments": {
      "type": "biginteger",
      "default": "0"
    },
    "publishedDate": {
      "type": "date"
    },
    "lastUpdated": {
      "type": "date"
    },
    "imageurl": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "mentor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author-course.author-course",
      "inversedBy": "course"
    },
    "feedback": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "course_id"
    }
  }
}
